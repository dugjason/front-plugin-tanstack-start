{
  "name": "front-plugin-tanstack-start",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 5174",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "deploy": "vercel pull --yes --environment=preview && vercel build && vercel deploy --prebuilt",
